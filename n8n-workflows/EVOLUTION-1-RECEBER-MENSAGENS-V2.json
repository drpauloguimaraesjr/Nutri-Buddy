    },
    {
      "parameters": {
        "method": "PUT",
        "url": "=https://firestore.googleapis.com/v1/projects/nutribuddy-2fc9c/databases/(default)/documents/whatsappConversations?documentId={{$node['Preparar Dados do Paciente'].json.conversationId}}",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "googleApi",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"fields\": {\n    \"patientId\": {\"stringValue\": \"{{$node['Preparar Dados do Paciente'].json.patientId}}\"},\n    \"patientName\": {\"stringValue\": \"{{$node['Preparar Dados do Paciente'].json.patientName}}\"},\n    \"patientPhone\": {\"stringValue\": \"{{$node['Extrair Dados da Mensagem'].json.phone}}\"},\n    \"prescriberId\": {\"stringValue\": \"{{$node['Preparar Dados do Paciente'].json.prescriberId}}\"},\n    \"status\": {\"stringValue\": \"active\"},\n    \"score\": {\n      \"mapValue\": {\n        \"fields\": {\n          \"totalScore\": {\"integerValue\": 50},\n          \"adherencePercentage\": {\"integerValue\": 50},\n          \"mealsLogged\": {\"integerValue\": 0},\n          \"correctMeals\": {\"integerValue\": 0},\n          \"consecutiveDays\": {\"integerValue\": 0},\n          \"badges\": {\"arrayValue\": {\"values\": []}}\n        }\n      }\n    },\n    \"lastMessage\": {\n      \"mapValue\": {\n        \"fields\": {\n          \"content\": {\"stringValue\": \"{{$node['Extrair Dados da Mensagem'].json.message}}\"},\n          \"timestamp\": {\"timestampValue\": \"{{new Date().toISOString()}}\"},\n          \"senderType\": {\"stringValue\": \"patient\"}\n        }\n      }\n    },\n    \"lastMessageAt\": {\"timestampValue\": \"{{new Date().toISOString()}}\"},\n    \"unreadCount\": {\"integerValue\": 1},\n    \"totalMessages\": {\"integerValue\": 1},\n    \"createdAt\": {\"timestampValue\": \"{{new Date().toISOString()}}\"},\n    \"updatedAt\": {\"timestampValue\": \"{{new Date().toISOString()}}\"}\n  }\n}",
        "options": {}
      },
      "id": "http-create-conversation",
      "name": "Criar Nova Conversa",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [2050, 200],
      "credentials": {
        "googleApi": {
          "id": "1",
          "name": "Google Service Account account"
        }
      }
    },
