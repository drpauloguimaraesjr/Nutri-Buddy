        "jsonBody": "={\n  \"conversationId\": \"{{$json.conversationId}}\",\n  \"patientName\": \"{{$json.patientName}}\",\n  \"urgency\": \"{{$json.urgency}}\",\n  \"tags\": {{JSON.stringify($json.tags)}}\n}",
        "options": {}
      },
      "id": "send-alert",
      "name": "Enviar Email de Alerta",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
